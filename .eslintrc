{
  "env": {
    "jest/globals": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "react/jsx-indent": [
      2,
      2
    ],
    "indent": [
      "error",
      2
    ],
    "semi": "error",
    "quotes": [
      2,
      "single"
    ],
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ]
  },
  "plugins": [
    "react",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended",
    "plugin:jest/style"
  ]
}